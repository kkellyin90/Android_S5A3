<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.funapp.session5assignment3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainCourse">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AndroidActivity"
            android:label="@string/title_activity_android"
            android:parentActivityName=".MainCourse"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.funapp.session5assignment3.MainCourse" />
        </activity>
        <activity
            android:name=".JavaActivity"
            android:label="@string/title_activity_java"
            android:parentActivityName=".MainCourse"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.funapp.session5assignment3.MainCourse" />
        </activity>
        <activity
            android:name=".FrontendActivity"
            android:label="@string/title_activity_frontend"
            android:parentActivityName=".MainCourse"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.funapp.session5assignment3.MainCourse" />
        </activity>
        <activity
            android:name=".LinuxActivity"
            android:label="@string/title_activity_linux"
            android:parentActivityName=".MainCourse"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.funapp.session5assignment3.MainCourse" />
        </activity>
    </application>

</manifest>